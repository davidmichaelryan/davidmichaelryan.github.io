@font-face {
    font-family: 'hipstelvetica-light'
    src: url('../images/benefit/hipstelvetica_light-webfont.eot')
    src: url('../images/benefit/hipstelvetica_light-webfont.eot?#iefix') format('embedded-opentype'),
         url('../images/benefit/hipstelvetica_light-webfont.woff') format('woff'),
         url('../images/benefit/hipstelvetica_light-webfont.ttf') format('truetype'),
         url('../images/benefit/hipstelvetica_light-webfont.svg#ao_lt_btlight') format('svg')
    font-weight: normal
    font-style: normal
}
@font-face {
    font-family: 'ao_lt_btlight'
    src: url('../images/benefit/ao-light-webfont.eot')
    src: url('../images/benefit/ao-light-webfont.eot?#iefix') format('embedded-opentype'),
         url('../images/benefit/ao-light-webfont.woff') format('woff'),
         url('../images/benefit/ao-light-webfont.ttf') format('truetype'),
         url('../images/benefit/ao-light-webfont.svg#ao_lt_btlight') format('svg')
    font-weight: normal
    font-style: normal
}

.benefit
  body
    padding: 0
    margin-bottom: 0
    position: relative
  #menu
    padding: 0 20px
  #container
    background: rgb(92,102,117)
    background: -moz-linear-gradient(top,  rgba(92,102,117,1) 0%, rgba(124,212,170,1) 100%)
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(92,102,117,1)), color-stop(100%,rgba(124,212,170,1)))
    background: -o-linear-gradient(top,  rgba(92,102,117,1) 0%,rgba(124,212,170,1) 100%)
    background: -ms-linear-gradient(top,  rgba(92,102,117,1) 0%,rgba(124,212,170,1) 100%) 
    background: linear-gradient(to bottom,  rgba(92,102,117,1) 0%,rgba(124,212,170,1) 100%)
    filter: unqoute("progid:DXImageTransform.Microsoft.gradient(startColorstr='#5c6675', endColorstr='#7cd4aa',GradientType=0)")
    padding-top: 20px

    section
      width: 900px
      margin: 0 auto

    h1, h2, h3, h4, h5, a, p
      font-family: "ao_lt_btlight", Helvetica, Arial, sans-serif
      text-transform: uppercase


  .intro
    height: 120px
    
    .aologo
      background-image: url('../images/benefit/whitelogo.png')
      background-repeat: no-repeat
      background-size: 120px 80px
      float: left
      height: 80px
      margin-top: 10px
      width: 120px
    h1
      display: inline-block
      font-size: 33px
      margin: 26px 0 0 3px
      position: relative
      text-align: center
      text-shadow:1px 1px 3px #000
      width: 675px
      z-index: 4
    .dmlogo
      background-image: url('../images/benefit/dmlogo.png')
      background-repeat: no-repeat
      background-size: 100px 100px
      float: right
      height: 100px
      width: 100px

  .video
    display: block
    height: 900px
    overflow: hidden
    position: relative
    top: -50px
    z-index: 0
    
    .t, h3
      font-family: 'hipstelvetica-light' !important
      position: absolute
      text-align: center
      text-shadow:1px 1px 3px #000
      width: 100%
      z-index: 2
    .t
      font-size: 64px
      top:175px
    h3
      font-size: 32px
      top:260px
    
    img 
      height: auto
      left: 0
      position: absolute
      top: 0
      width: 100%
      z-index: 3

    video
      left: 0
      position: absolute
      top: 100px
      z-index: 1
      -webkit-clip-path: polygon(78px 62px, 820px 62px, 450px 755px)
      clip-path: url(#clipPolygon)
      -webkit-mask: url(unquote('data:image/svg+xml;charset=utf-8;base64,')) no-repeat
    .mask 
      background-image:url('http://eprogramers.com/triangle.gif')
      height: 720px
      position: absolute
      width: 100%
      z-index: 2
  

#cheers
    -webkit-transform: rotate(59deg)
    -moz-transform: rotate(59deg)
    -o-transform: rotate(59deg)
    -ms-transform: rotate(59deg)
    transform: rotate(59deg)
    font-size: 48px
    position: absolute
    z-index:4
    text-shadow:1px 1px 3px #000
    top:600px
    left:-40px


#magic
    -webkit-transform: rotate(-59deg)
    -moz-transform: rotate(-59deg)
    -o-transform: rotate(-59deg)
    -ms-transform: rotate(-59deg)
    transform: rotate(-59deg)
    font-size: 48px
    position: absolute
    z-index:4
    text-shadow:1px 1px 3px #000
    top:600px
    right:40px;